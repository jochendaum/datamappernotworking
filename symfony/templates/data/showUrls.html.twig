                <tr>
                    <td>Historical weekly SMP price comparisons</td>
                    <td><a target="_new" href="{{ path('data_historical_weekly_smp_price_comparisons') }}">{{app.request.getSchemeAndHttpHost()}}{{ path('data_historical_weekly_smp_price_comparisons') }}</a></td>
